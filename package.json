{
  "name": "wordfind",
  "version": "1.0.0",
  "description": "A port of Bill Scheidel's wordfind for node and npm",
  "main": "index.js",
  "bin": {
    "wordfind": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "build": "sh scripts/build.sh",
    "watch": "nodemon --quiet",
    "test": "tape test/**/*.js | tap-notify | tap-diff",
    "t-watch": "nodemon --quiet -x 'npm run test'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/brevity/wordfind.git"
  },
  "keywords": [
    "word",
    "search",
    "breeder",
    "game",
    "kids"
  ],
